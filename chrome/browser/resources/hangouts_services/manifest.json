{
  "name": "Hangouts Services",
  "version": "1.0",
  "manifest_version": 2,
  "icons": {
    "128": "icon_128.png"
  },
  "externally_connectable": {
    "matches": ["*://*.google.com/hangouts*"]
  },
  "background": {
    "scripts": ["thunk.js"],
    "persistent": false
  },
  "permissions": [
    "desktopCapture",
    "system.cpu",
    "webrtcAudioPrivate",
    "webrtcLoggingPrivate"
  ]
}
