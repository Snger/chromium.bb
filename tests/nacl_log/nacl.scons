Import('env')

# tests in this directory contain c++ which does not work on pnacl/x86
if env.Bit('bitcode'):
  Return()

env.Replace(EXTRA_LIBS=['platform', 'gio', 'pthread'])

nacl_log_nexe = env.ComponentProgram('nacl_log', 'nacl_log.c')


node = env.CommandSelLdrTestNacl(
    'nacl_log_test.out',
    nacl_log_nexe,
    )
env.AddNodeToTestSuite(node,
                       ['small_tests', 'sel_ldr_tests'],
                       'run_nacl_log_test')
