Import('env')

env.Prepend(CPPDEFINES=['XP_UNIX'])

ppapi_bad_nexe = env.ComponentProgram('ppapi_bad.nexe',
                                      ['ppapi_bad.cc', ],
                                      EXTRA_LIBS=['${PPAPI_LIBS}',
                                                  'platform',
                                                  'pthread',
                                                  'gio'])

env.Publish('ppapi_bad.nexe', 'run', ['ppapi_bad.html'])
