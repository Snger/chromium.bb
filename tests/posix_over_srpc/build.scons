Import('env')

if env['TARGET_PLATFORM'] != 'LINUX':
    Return()

launcher = env.ComponentProgram(
    'posix_over_srpc_launcher', ['launcher_main.cc', 'launcher.cc'],
    EXTRA_LIBS=[
        'posix_over_srpc_linux',
        'nonnacl_srpc',
        'nonnacl_util',
        'nrd_xfer',
        'platform',
        'google_nacl_imc_c',
        'gio',
        'dl'])
