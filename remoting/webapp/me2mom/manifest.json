{
  "name": "Chromoting Me2Mom",
  "version": "1.0",
  "description": "Me2Mom-style remote support in Chrome.",
  "app": {
    "launch": {
      "local_path": "choice.html"
    }
  },
  "background_page": "background.html",
  "icons": {
    "128": "chromoting128.png"
  },
  "permissions": [
    "tabs",
    "https://www.google.com/accounts/*"
  ],
  "PLUGINS": "PLACEHOLDER"
}
