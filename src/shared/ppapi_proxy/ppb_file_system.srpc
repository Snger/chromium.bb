# RPC methods used to implement PPB_FileSystem_Dev calls from the plugin.
# See ppapi/c/dev/ppb_file_system_dev.h for interface details.

{
  'name': 'PpbFileSystemDevRpc',
  'rpcs': [
          {'name': 'PPB_FileSystem_Dev_Create',
           'inputs': [['instance', 'PP_Instance'],
                      ['file_system_type', 'int32_t'],   # PP_FileSystemType_Dev
                      ],
           'outputs': [['resource', 'PP_Resource'],
                       ]
          },
          {'name': 'PPB_FileSystem_Dev_IsFileSystem',
           'inputs': [['resource', 'PP_Resource'],
                      ],
           'outputs': [['success', 'int32_t'],  # PP_Bool
                       ]
          },
          {'name': 'PPB_FileSystem_Dev_Open',
           'inputs': [['file_system', 'PP_Resource'],
                      ['expected_size', 'int64_t'],
                      ['callback_id', 'int32_t']
                      ],
           'outputs': [['pp_error', 'int32_t'],
                       ]
          },
          {'name': 'PPB_FileSystem_Dev_GetType',
           'inputs': [['file_system', 'PP_Resource'],
                      ],
           'outputs': [['type', 'int32_t'],  # PP_FileSystemType_Dev
                       ],
          }
          ]
}

