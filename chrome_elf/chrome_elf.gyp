{
  'variables': {
    'chromium_code': 1,
  },
  'includes': [
    '../build/win_precompile.gypi',
    '../chrome/version.gypi',
  ],
  'targets': [
    {
      'target_name': 'chrome_elf',
      'type': 'shared_library',
      'include_dirs': [
        '..',
      ],
      'sources': [
        'chrome_elf.def',
        'chrome_elf_main.cc',
        'chrome_elf_main.h',
      ],
      'msvs_settings': {
        'VCLinkerTool': {
          'BaseAddress': '0x35000000',
          # Set /SUBSYSTEM:WINDOWS (for consistency).
          'SubSystem': '2',
        },
      },
    },
  ],
}